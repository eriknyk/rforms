{
  "name": "rforms",
  "version": "0.0.1",
  "description": "A form input builder and validator for React JS (based on formsy-react)",
  "repository": {
    "type": "git",
    "url": "https://github.com/eriknyk/rforms.git"
  },
  "author": "Erik Amaru Ortiz",
  "license": "MIT",
  "keywords": [
    "react",
    "form",
    "forms",
    "validation",
    "react-component"
  ],
  "scripts": {
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "test": "babel-node testrunner",
    "examples": "webpack-dev-server --config examples/webpack.config.js --content-base examples",
    "prepublish": "babel ./src/ -d ./lib/"
  },
  "main": "lib/index.js",
  "dependencies": {
    "form-data-to-object": "^0.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "jsdom": "^6.5.1",
    "nodeunit": "^0.9.1",
    "react": "15.2.1",
    "react-addons-pure-render-mixin": "^15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.2.1",
    "sinon": "^1.17.3",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "react": "15.2.1"
  },
  "bugs": {
    "url": "https://github.com/eriknyk/rforms/issues"
  },
  "homepage": "https://github.com/eriknyk/rforms#readme",
  "directories": {
    "example": "examples"
  }
}
